{
    "name": "rocketslab/neewton",
    "type": "project",
    "description": "The InertiaJs Module Manager.",
    "keywords": ["package", "laravel", "inertiajs"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "inertiajs/inertia-laravel": "^0.4.3",
        "illuminate/support": "^8.0",
        "illuminate/console": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "RocketsLab\\Neewton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RocketsLab\\Neewton\\NeewtonServiceProvider"
            ],
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
